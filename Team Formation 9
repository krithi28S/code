import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner (System.in);
        int a = scan.nextInt();
        int b = scan.nextInt();
        int c = a/b;
        int d = a%b;
        System.out.println("Total teams formed: " + c);
        System.out.println("Players left out: " + d);
        
        
        
    }
}
